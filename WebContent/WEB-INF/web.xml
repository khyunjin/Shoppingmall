<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Shoppingmall</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>UserInsertServlet</servlet-name>
  	<servlet-class>user.UserInsertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserInsertServlet</servlet-name>
  	<url-pattern>/userinsert</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>UserUpdateServlet</servlet-name>
  	<servlet-class>user.UserUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserUpdateServlet</servlet-name>
  	<url-pattern>/userupdate</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<servlet-class>user.UserLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<url-pattern>/userlogin</url-pattern>
  </servlet-mapping>
    <servlet>
  	<servlet-name>UserIdFindServlet</servlet-name>
  	<servlet-class>user.UserIdFindServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserIdFindServlet</servlet-name>
  	<url-pattern>/useridfind</url-pattern>
  </servlet-mapping>
    <servlet>
  	<servlet-name>UserPwdFindServlet</servlet-name>
  	<servlet-class>user.UserPwdFindServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserPwdFindServlet</servlet-name>
  	<url-pattern>/userpwdfind</url-pattern>
  </servlet-mapping>
    <servlet>
  	<servlet-name>ProductListServlet</servlet-name>
  	<servlet-class>product.ProductListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProductListServlet</servlet-name>
  	<url-pattern>/productlist</url-pattern>
  </servlet-mapping>
</web-app>
